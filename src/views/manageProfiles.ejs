<% var title = 'manageCategories' %>
<% include head %>
<% include header %>
<div class="w3-container w3-brown">
  <h2 class="w3-lobster">Gestion des Cat√©gories</h2>
</div>
<% if(users != null){ %>
<div class="w3-card-4 w3-margin">
  <ul class="w3-ul">
    <% users.forEach(function(selectedUser){ %>
      <% if(selectedUser.id != user.id){ %>
      <form class="w3-container" method="post" action="">
        <input type="text" name="nameCategorie" value="<%= selectedUser.nom %>">
        <input type="submit" name="delete" value="delete">
      </form>
      <% } %>
    <% }); %>
  </ul>
</div>
<% } %>
